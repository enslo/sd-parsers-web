{
  "name": "sd-parsers-web",
  "version": "1.0.0",
  "description": "Browser-only library to read metadata from Stable Diffusion images",
  "type": "module",
  "main": "./dist/sd-parsers-web.umd.js",
  "module": "./dist/sd-parsers-web.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/sd-parsers-web.es.js",
      "require": "./dist/sd-parsers-web.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "dev": "vite build --watch",
    "test": "jest",
    "sync-version": "./scripts/sync-version.sh"
  },
  "keywords": [
    "stable-diffusion",
    "metadata",
    "image-parsing",
    "browser",
    "web",
    "ai",
    "automatic1111",
    "comfyui",
    "fooocus",
    "invokeai",
    "novelai"
  ],
  "author": "Ernest Croft",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "exifr": "^7.1.0",
    "png-chunks-extract": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/png-chunks-extract": "^1.0.2",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0"
  },
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/enslo/sd-parsers-web.git"
  }
}